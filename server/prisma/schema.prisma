generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  firstName String
  isAdmin   Boolean @default(false)
  email     String
  lastName  String
  password  String
  role      Role    @default(Employee)
}

enum Role {
  Employee
  Company
}
